"language": "GDScript",
"path": "res://scripts/spell/dagger/dagger_manager.gd"
}, {
"base": "Node2D",
"class": "EnemySpawner",
"language": "GDScript",
"path": "res://scripts/management/enemy_spawner.gd"
}, {
